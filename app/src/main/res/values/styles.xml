<resources>

    <style name="AppTheme" parent="Theme.AppCompat"/>

    <style name="LauncherTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorPrimary">@android:color/darker_gray</item>
        <item name="android:colorPrimaryDark">@android:color/darker_gray</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
        <item name="android:windowTranslucentStatus">true</item>

        <item name="swirl_ridgeColor">@android:color/darker_gray</item>
        <item name="swirl_errorColor">@android:color/holo_red_dark</item>
    </style>

    <style name="WebTheme" parent="android:Theme.Material.NoActionBar"/>

    <style name="SettingsTheme" parent="Theme.AppCompat.Light.NoActionBar">
    </style>

</resources>
